
incl quad;

var positives::float::array {float:};
var negatives::float::array {float:};

set positives[^$] (@quad#1#0#-1)[0];
set negatives[^$] (@quad#1#0#-1)[1];

set positives[^$] (@quad#2#0#-2)[0];
set negatives[^$] (@quad#2#0#-2)[1];

set positives[^$] (@quad#1#-2#1)[0];
set negatives[^$] (@quad#1#-2#1)[1];

out `positives` + '\n' + `negatives`;
